%header
  %h1 Загрузка цифровых копий

= form_for @folder do |f|
  = render 'form', :f => f
  .actions
  = f.submit 'Сохранить'

#pictures
  = render :partial => '/pictures/picture', :collection => @folder.pictures

= form_for :picture, :url => folder_pictures_path(@folder),
:html => { :class => 'pics_upload', :multipart => true} do |f|
  = f.file_field :image, :multiple => true, :name => 'picture[image]'
  %table.upload_files

= link_to 'Назад', folders_path
